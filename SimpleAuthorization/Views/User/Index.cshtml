@model SimpleAuthorization.Models.User
@{
    ViewData["Title"] = $"{Model.Email}";
}
<table class="table table-info">

    <tbody>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Город</th>
            <th>Телефон</th>
            <th>email</th>
            <th>Пароль</th>
        </tr>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Fname</td>
            <td>@Model.Lname</td>
            <td>@Model.City</td>
            <td>@Model.Phone</td>
            <td>@Model.Email</td>
            <td>@Model.PasswordHash</td>
        </tr>
    </tbody>
</table>