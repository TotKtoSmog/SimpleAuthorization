@model IEnumerable<SimpleAuthorization.Models.User>
@{
    ViewData["Title"] = "Пользователи";
}
<table class="table table-info">

    <tbody>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Город</th>
            <th>Телефон</th>
            <th>email</th>
            <th>Пароль</th>
        </tr>
        @foreach (var mes in Model)
        {
            
            <tr>
                <td>@mes.Id</td>
                <td>@mes.Fname</td>
                <td>@mes.Lname</td>
                <td>@mes.City</td>
                <td>@mes.Phone</td>
                <td>@mes.Email</td>
                <td>@mes.PasswordHash</td>
            </tr>
        }
    </tbody>
    <caption>>:^) Всего: @Model.Count()</caption>
</table>